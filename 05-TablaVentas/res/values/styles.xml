<resources>

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
    <color name="pureblack">#000000</color>
    <color name="puregreen">#185529</color>
    <color name="purered">#660E0E</color>
    <color name="purewhite">#FFFFFF</color>
    <color name="translucid_white">#77FFFFFF</color>
    <color name="purepurple">#56046B</color>
    <color name="puresea">#075b66</color>
    <color name="pureorange">#f29423</color>
    <color name="translucid_orange">#88f29423</color>
    <color name="translucid_sea">#99389eaa</color>
	<color name="translucid_green">#9996CF76</color>
	<color name="translucid_red">#99E65D5D</color>
    <color name="translucid_black">#99000000</color>

</resources>
